<p id="notice"><%= notice %></p>

<h1>Listing Daily Boards</h1>

<table>
  <thead>
    <tr>
      <th>User id</th>
      <th>Create time</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_boards.each do |daily_board| %>
      <tr>
        <td><%= daily_board.user_id %></td>
        <td><%= daily_board.create_time %></td>
        <td><%= daily_board.title %></td>
        <td><%= daily_board.content %></td>
        <td><%= link_to 'Show', daily_board %></td>
        <td><%= link_to 'Edit', edit_daily_board_path(daily_board) %></td>
        <td><%= link_to 'Destroy', daily_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily board', new_daily_board_path %>
